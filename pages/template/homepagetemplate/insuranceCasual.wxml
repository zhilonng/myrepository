<template name="insuranceCasual">
<view wx:for="{{productName}}" data-index="{{index}}" bindtap="turnIntoCaculaPage"  class="insuranceContainer">
<image src="{{productInfo[index].goods_type_pic}}" class="insurancePhoto" mode="aspectFill"/>
<view class="insuranceNameAndPrice" >
<text class="insuranceName">{{item}}</text>
<text class="insurancePrice">￥{{productInfo[index].goods_type_price}}起</text>
</view>
</view>
</template>