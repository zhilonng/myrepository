/* pages/homePage/servicepurchase/servicepurchase.wxss */
page{
    background: #eeeeee;
}
.logoContainer{
    width: 100%;
    display: flex;
    flex-direction: row;
    margin-top: 25rpx;
}
.logo{
    width: 100rpx;
    height: 90rpx;
    margin-right: 10rpx;
    margin-left: 15rpx;
}
.logo_mami_yanjiuyuan{
    font-size: 40rpx;
    color: #353535;
    font-weight: bold;
    margin-left: 10rpx;
}
.logoLabelContainer{
    width: auto;
    float: right;
    margin-left: 200rpx;
}
.logo_introduce{
    font-size: 30rpx;
    color: #888888;
}
.logo_label_container{
    display: flex;
    flex-direction: row;
}
.logo_label{
    width: 20rpx;
    height: 20rpx;
    margin-top: 2rpx;
}
.logo_label_introduce{
    font-size: 24rpx;
    color: #888888;
}

/*保险服务*/
.insruanceContainer{
    width: 96%;
    height: auto;
    background: #FFF;
    margin: 20rpx 2% 0rpx 2%;
    border-radius: 15rpx;
}
.insurance_service_consule{
    display: flex;
    flex-direction: row;
}
.hover_insurance_service_consule{
    display: flex;
    flex-direction: row;
    background: #D9D9D9;
    border-radius: 15rpx;
}
.img_consule{
    margin: 20rpx 10rpx 0rpx 20rpx;
    width: 150rpx;
    height: 150rpx;
}
.tx_insrance_consule{
    margin-top: 20rpx;
    margin-bottom: 6rpx;
    font-weight:800;
    color:#12b7f5;
}
.tx_consule_content{
    font-size: 25rpx;
    color: #666;
}
.tx_consule_money{
    height:30rpx;
    margin-top:20rpx;
    margin-left: 120rpx;
    border:1px solid #12b7f5;
    padding:2px 5px;
    border-radius:30rpx;
    color:#12b7f5;
    font-size:25rpx;
    text-align:center;
}
.ic_turnTo{
    width: 50rpx;
    height: 50rpx;
    color: #999999;
    font-size: 60rpx;
    font-weight: bold;
    margin-top: 10rpx;
    margin-left: 135rpx;
}
.tp_insuranceClassify_line{
    width: 100%;
    height: 3rpx;
    background: #F5F5F5;
}